# Example environment configuration for E2E tests
# Copy this to .env.test and fill in your values

# Test environment: local, dev, or prod
TEST_ENV=local

# API key for dev/prod environments
AIRWEAVE_API_KEY=

# Required credentials
STRIPE_API_KEY=sk_test_...
OPENAI_API_KEY=  # Optional but recommended

# OAuth test tokens
TEST_NOTION_TOKEN=
TEST_GOOGLE_CLIENT_ID=
TEST_GOOGLE_CLIENT_SECRET=

# Auth provider configuration (must be composio)
TEST_AUTH_PROVIDER_NAME=composio
TEST_COMPOSIO_API_KEY=
TEST_COMPOSIO_ASANA_AUTH_CONFIG_ID=
TEST_COMPOSIO_ASANA_ACCOUNT_ID=
TEST_COMPOSIO_GMAIL_AUTH_CONFIG_ID=
TEST_COMPOSIO_GMAIL_ACCOUNT_ID=



# Test behavior settings
SKIP_STARTUP=false
STRICT_MODE=false

# Timeouts
DEFAULT_TIMEOUT=30
SYNC_TIMEOUT=60

# Parallelization
MAX_WORKERS=4
